QToolButton {
    background-color: transparent;
    border: 1px transparent #76797C;
    border-radius: 2px;
    margin: 3px;
    padding: 5px;
}

QToolButton[popupMode=&quot;1&quot;] {
    /* only for MenuButtonPopup */
    padding-right: 20px;
